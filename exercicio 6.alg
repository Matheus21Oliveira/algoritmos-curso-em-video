algoritmo "exercicio 6"

var
numero: inteiro
sucessor: inteiro
antecessor: inteiro
inicio
escreval ("escolha um numero inteiro")
leia (numero)
sucessor <- numero + 1
antecessor <- numero - 1
escreval ("o sucessor do" , (numero) ," é ", (sucessor))
escreva (" o antecessor do" , (numero) ," é ", (antecessor))
fimalgoritmo
